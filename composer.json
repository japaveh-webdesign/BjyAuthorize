  "minimum-stability": "dev",
  "require": {
    "php": "^7.1",
    "zendframework/zend-permissions-acl": "~2.5 || ~3.0",
    "zendframework/zend-mvc": "~2.5 || ~3.0",
    "zendframework/zend-eventmanager": "~2.5 || ~3.0",
    "zendframework/zend-servicemanager": "~2.5 || ~3.0",
    "zendframework/zend-http": "~2.5 || ~3.0",
    "zendframework/zend-view": "~2.5 || ~3.0",
    "zendframework/zend-cache": "~2.5 || ~3.0"
  },
  "autoload": {
    "psr-0": {
